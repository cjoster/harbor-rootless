[Container]
ContainerName=harbor-trivy-adapter
Pod=harbor.pod
Image=docker.io/goharbor/trivy-adapter-photon:v2.13.0
DropCapability=ALL
Environment=harbor-trivy-adapter.env
Network=harbor.network
Volume=/var/lib/harbor/data/trivy-adapter/trivy:/home/scanner/.cache/trivy
Volume=/var/lib/harbor/data/trivy-adapter/reports:/home/scanner/.cache/reports
Volume=/etc/harbor/harbor-rootless/trust-certificates:/harbor_cust_cert

[Service]
Restart=always
