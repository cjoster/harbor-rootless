[Container]
ContainerName=harbor-registryctl
Pod=harbor.pod
Image=docker.io/goharbor/harbor-registryctl:v2.13.0
DropCapability=ALL
AddCapability=CHOWN SETGID SETUID
EnvironmentFile=harbor-registryctl.env
Network=harbor.network
Volume=${HOME}/data/registry:/storage:z
Volume=${HOME}/harbor-registry/:/etc/registry/:z
Volume=${HOME}/harbor-registryctl/config.yml:/etc/registryctl/config.yml:z
Volume=${HOME}/trust-certificates:/harbor_cust_cert:z

[Service]
#Restart=always
