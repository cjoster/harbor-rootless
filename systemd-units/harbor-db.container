[Container]
User=harbor
Group=harbor
ContainerName=harbor-db
Pod=harbor.pod
Image=docker.io/goharbor/harbor-db:v2.13.0
DropCapability=ALL
AddCapability=CHOWN DAC_OVERRIDE SETGID SETUID
EnvironmentFile=harbor-db.env
Network=harbor.network
Volume=/var/lib/harbor/data/database:/var/lib/postgresql/data:Z

[Service]
Restart=always
